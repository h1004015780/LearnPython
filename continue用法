public class Continue {
	public static void main(String[] args) {
		int count = 0 ;//定义计数器
		for (int i = 100;i < 150;i++){
			
			if(i % 3 == 0){
				
				continue;
				
			}
			//否则输出该数
			System.out.println(i + ",");
			count++;
			if(count % 5 ==0){
				
				System.out.println();
			}
			
		}		
		//输出不能%3的数
		
	}

}
